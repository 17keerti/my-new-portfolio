<nav class="uk-navbar-container" uk-navbar>
  <div class="uk-navbar-left">
    <a class="uk-navbar-item uk-logo" href="#">Keerti</a>
  </div>
  <div class="uk-navbar-right uk-visible@s">
    <ul class="uk-navbar-nav">
      <li><a href="/"><span uk-icon="home"></span>Home</a></li>
      <li><a href="/"><span uk-icon="user"></span>About</a></li>
      <li><a href="/"><span uk-icon="folder"></span>Projects</a></li>
      <li><a href="#contactme"><span uk-icon="user"></span>Contact me</a></li>
    </ul>
  </div>
  <a class="uk-navbar-toggle uk-hidden@s" uk-toggle="target: #sidenav" uk-navbar-toggle-icon></a>
</nav>
<div id="sidenav" uk-offcanvas="flip: true" class="uk-offcanvas">
  <div class="uk-offcanvas-bar">
    <ul class="uk-navbar-nav">
      <li><a href="/"><span uk-icon="home"></span>Home</a></li>
      <li><a href="/"><span uk-icon="user"></span>About</a></li>
      <li><a href="/"><span uk-icon="folder"></span>Projects</a></li>
